{
    "name": "phpbench/pipeline",
    "description": "Composable micro benchhmarking framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1",
        "symfony/options-resolver": "^4.0"
    },
    "require-dev": {
        "infection/infection": "^0.8.1",
        "phpstan/phpstan": "^0.8.5",
        "phpunit/phpunit": "^6.0",
        "symfony/process": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "PhpBench\\Pipeline\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpBench\\Pipeline\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": ["bin/phpbench"]
}
