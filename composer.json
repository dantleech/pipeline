{
    "name": "phpbench/framework",
    "description": "PHP Benchmarking Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1",
        "markrogoyski/math-php": "^0.38.0",
        "myclabs/deep-copy": "^1.7"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.8.5",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "PhpBench\\Framework\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpBench\\Framework\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": ["bin/phpbench"]
}
